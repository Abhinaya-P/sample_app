<% provide(:title,'Sign In') %>
<h1>Sign In</h1><div class="row" >
	<div class="span6 offset3">
		<%= form_for(:session, url: sessions_path ) do | s |%>
		   <%= s.label :email %>
		   <%= s.text_field :email %>

		   <%= s.label :password %>
		   <%= s.password_field :password %>

		   <%= s.submit "Sign In", class: "btn btn-large btn-primary" %> 
		<% end %>
     <p>New user? <%= link_to "Sign up now!", new_user_path %></p>
	</div>
</div>